{
    "macros": [
        "ARM_UC_USE_PAL_BLOCKDEVICE=1",
        "MBED_CLOUD_CLIENT_UPDATE_STORAGE=ARM_UCP_FLASHIAP_BLOCKDEVICE"
    ],
    "target_overrides": {
        "*": {
            "target.features_add"               : ["BOOTLOADER", "STORAGE"],
            "platform.stdio-baud-rate"          : 115200,
            "platform.stdio-convert-newlines"   : true,
            "platform.stdio-buffered-serial"    : true,
            "mbed-client.event-loop-size"       : 1024,
            "nanostack-hal.event_loop_thread_stack_size": 8192,
            "update-client.storage-address"     : "(1024*1024*64)",
            "update-client.storage-size"        : "(1024*1024*2)",
            "update-client.storage-locations"   : 1,
            "mbed-trace.enable": null,
            "nsapi.default-wifi-security"       : "WPA_WPA2",
            "nsapi.default-wifi-ssid"           : "\"SSID\"",
            "nsapi.default-wifi-password"       : "\"Password\""
        },
        "STM_EMAC": {
            "lwip.pbuf-pool-size" : 16,
            "lwip.mem-size"       : 12500
        },
        "K64F": {
            "target.network-default-interface-type" : "ETHERNET",
            "update-client.bootloader-details"      : "0x00007188",
            "update-client.application-details"     : "(40*1024)"
        },
        "K66F": {
            "target.network-default-interface-type" : "ETHERNET",
            "update-client.bootloader-details"      : "0x00007188",
            "update-client.application-details"     : "(40*1024)"
        },
        "NUCLEO_F429ZI": {
            "target.network-default-interface-type" : "ETHERNET",
            "update-client.bootloader-details"      : "0x080078CC",
            "update-client.application-details"     : "(0x08000000+64*1024)"
        },
        "UBLOX_EVK_ODIN_W2": {
            "target.network-default-interface-type" : "WIFI",
            "update-client.bootloader-details"      : "0x08007300",
            "update-client.application-details"     : "(0x08000000+64*1024)",
            "target.lse_available"                  : 0,
            "target.macros_remove"                  : ["MBEDTLS_CONFIG_HW_SUPPORT"]
        },
        "NUCLEO_F411RE": {
            "client_app.mbedtls-user-config-file"      : "\"mbedTLSConfig_mbedOS_SW_TRNG.h\"",
            "client_app.pal-user-defined-configuration": "\"sotp_non_trng_config_MbedOS.h\"",
            "target.network-default-interface-type" : "WIFI",
            "drivers.uart-serial-rxbuf-size"        : 1024,
            "drivers.uart-serial-txbuf-size"        : 1024,
            "update-client.bootloader-details"      : "(0x08000000+30*1024)",
            "update-client.application-details"     : "(0x08000000+64*1024)",
            "events.shared-stacksize"               : 2048,
            "idw0xx1.provide-default": true,
            "idw0xx1.tx": "PA_9",
            "idw0xx1.rx": "PA_10",
            "target.macros_remove"                  : ["MBEDTLS_CONFIG_HW_SUPPORT"]
        },
        "NUMAKER_PFM_NUC472": {
            "target.network-default-interface-type"     : "ETHERNET",
            "target.bootloader_img"                     : "tools/mbed-bootloader-nuc472-block_device-sotp-v3.5.0.bin",
            "target.header_offset"                      : "0x12000",
            "target.app_offset"                         : "0x12400",
            "target.components_add"                     : ["NUSD"],
            "update-client.bootloader-details"          : "0x0",
            "update-client.application-details"         : "0x12000",
            "update-client.storage-address"             : "(1024*1024*64)",
            "update-client.storage-size"                : "((MBED_ROM_START + MBED_ROM_SIZE - APPLICATION_ADDR) * MBED_CONF_UPDATE_CLIENT_STORAGE_LOCATIONS)",
            "update-client.storage-locations"           : 1,

            "mbed-cloud-client.update-storage"          : "ARM_UCP_FLASHIAP_BLOCKDEVICE",
            "mbed-cloud-client.external-sst-support"    : null,

            "client_app.sotp-section-1-address"         : "0x10000",
            "client_app.sotp-section-1-size"            : "0x1000",
            "client_app.sotp-section-2-address"         : "0x11000",
            "client_app.sotp-section-2-size"            : "0x1000",            

            "platform.stack-stats-enabled"              : true,
            "platform.heap-stats-enabled"               : true,

            "target.macros_add": [
                "ESP8266_AT_SEL=ESP8266_AT_EXTERN"
            ]
        },
        "NUMAKER_PFM_M487": {
            "target.network-default-interface-type"     : "ETHERNET",
            "target.bootloader_img"                     : "tools/mbed-bootloader-m487-block_device-sotp-v3.5.0.bin",
            "target.header_offset"                      : "0x12000",
            "target.app_offset"                         : "0x12400",
            "target.components_add"                     : ["NUSD"],
            "update-client.bootloader-details"          : "0x0",
            "update-client.application-details"         : "0x12000",
            "update-client.storage-address"             : "(1024*1024*64)",
            "update-client.storage-size"                : "((MBED_ROM_START + MBED_ROM_SIZE - APPLICATION_ADDR) * MBED_CONF_UPDATE_CLIENT_STORAGE_LOCATIONS)",
            "update-client.storage-locations"           : 1,

            "mbed-cloud-client.update-storage"          : "ARM_UCP_FLASHIAP_BLOCKDEVICE",
            "mbed-cloud-client.external-sst-support"    : null,

            "client_app.sotp-section-1-address"         : "0x10000",
            "client_app.sotp-section-1-size"            : "0x1000",
            "client_app.sotp-section-2-address"         : "0x11000",
            "client_app.sotp-section-2-size"            : "0x1000",

            "platform.stack-stats-enabled"              : true,
            "platform.heap-stats-enabled"               : true,

            "target.macros_add": [
                "ESP8266_AT_SEL=ESP8266_AT_EXTERN"
            ]
        },
        "NUMAKER_IOT_M487": {
            "target.network-default-interface-type"     : "WIFI",
            "target.bootloader_img"                     : "tools/mbed-bootloader-m487_iot-block_device-sotp-v3.5.0.bin",
            "target.header_offset"                      : "0x12000",
            "target.app_offset"                         : "0x12400",
            "target.components_add"                     : ["NUSD"],
            "update-client.bootloader-details"          : "0x0",
            "update-client.application-details"         : "0x12000",
            "update-client.storage-address"             : "(1024*1024*64)",
            "update-client.storage-size"                : "((MBED_ROM_START + MBED_ROM_SIZE - APPLICATION_ADDR) * MBED_CONF_UPDATE_CLIENT_STORAGE_LOCATIONS)",
            "update-client.storage-locations"           : 1,

            "mbed-cloud-client.update-storage"          : "ARM_UCP_FLASHIAP_BLOCKDEVICE",
            "mbed-cloud-client.external-sst-support"    : null,

            "drivers.uart-serial-rxbuf-size"            : 1024,
            "drivers.uart-serial-txbuf-size"            : 1024,

            "esp8266.tx"                                : "PH_8",
            "esp8266.rx"                                : "PH_9",
            "esp8266.rts"                               : "NC",
            "esp8266.cts"                               : "NC",
            "esp8266.rst"                               : "PH_3",
            "esp8266.provide-default"                   : false,

            "client_app.sotp-section-1-address"         : "0x10000",
            "client_app.sotp-section-1-size"            : "0x1000",
            "client_app.sotp-section-2-address"         : "0x11000",
            "client_app.sotp-section-2-size"            : "0x1000",

            "platform.stack-stats-enabled"              : true,
            "platform.heap-stats-enabled"               : true,

            "target.macros_add": [
                "ESP8266_AT_SEL=ESP8266_AT_ONBOARD"
            ]
        },
        "NUMAKER_PFM_M2351": {
            "target.network-default-interface-type"     : "WIFI",
            "target.bootloader_img"                     : "tools/mbed-bootloader-m2351-block_device-sotp-v3.5.0.hex",
            "target.header_offset"                      : "0x12000",
            "target.app_offset"                         : "0x12400",
            "target.components_add"                     : ["NUSD"],
            "update-client.bootloader-details"          : "0x0",
            "update-client.application-details"         : "0x10022000",
            "update-client.storage-address"             : "(1024*1024*64)",
            "update-client.storage-size"                : "((MBED_ROM_START + MBED_ROM_SIZE - APPLICATION_ADDR) * MBED_CONF_UPDATE_CLIENT_STORAGE_LOCATIONS)",
            "update-client.storage-locations"           : 1,

            "mbed-cloud-client.update-storage"          : "ARM_UCP_FLASHIAP_BLOCKDEVICE",
            "mbed-cloud-client.external-sst-support"    : null,

            "led-pinname"                               : "LED1",

            "drivers.uart-serial-rxbuf-size"            : 1024,
            "drivers.uart-serial-txbuf-size"            : 1024,

            "esp8266.tx"                                : "PD_1",
            "esp8266.rx"                                : "PD_0",
            "esp8266.rts"                               : "PD_3",
            "esp8266.cts"                               : "PD_2",
            "esp8266.rst"                               : "NC",
            "esp8266.provide-default"                   : false,

            "client_app.sotp-section-1-address"         : "0x10020000",
            "client_app.sotp-section-1-size"            : "0x1000",
            "client_app.sotp-section-2-address"         : "0x10021000",
            "client_app.sotp-section-2-size"            : "0x1000",

            "target.mbed_rom_start":    "0x10010000",
            "target.mbed_rom_size":     "0x70000",
            "target.mbed_ram_start":    "0x30002000",
            "target.mbed_ram_size":     "0x16000",

            "platform.stack-stats-enabled"              : true,
            "platform.heap-stats-enabled"               : true,

            "target.macros_add": [
                "ESP8266_AT_SEL=ESP8266_AT_ONBOARD"
            ]
        }
    },
    "config": {
        "developer-mode": {
            "help"      : "Enable Developer mode to skip Factory enrollment",
            "options"   : [null, 1],
            "value"     : 1
        },
        "button-pinname": {
            "help"      : "PinName for button.",
            "value"     : "BUTTON1"
        },
        "led-pinname"   : {
            "help"      : "PinName for led, which is attached to led blink resource.",
            "value"     : "LED_RED"
        }
    }
